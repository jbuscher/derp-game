{
  "name": "derp-game",
  "version": "0.0.1",
  "description": "Test game using socketio, react, and node",
  "author": {
    "name": "John Buscher",
    "email": "john.buscher12@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbuscher/derp-game"
  },
  "main": "src/server.js",
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/env"
    ]
  },
  "scripts": {
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"webpack -wd\" \"nodemon server.js\"",
    "dev:server": "cd client && npm run build && cd .. && npm start",
    "start": "node server.js",
    "build-dev": "webpack -d --mode development",
    "build-prod": "webpack -p --mode production",
    "heroku-postbuild": "cd client && yarn install && yarn install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "nodemon": "^1.18.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "socket.io": "^2.1.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "webpack-cli": "^3.1.0"
  },
  "keywords": [
    "node",
    "express",
    "socketio"
  ]
}
